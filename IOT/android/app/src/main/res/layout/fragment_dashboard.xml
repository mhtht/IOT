<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment">

    <ImageView
        android:id="@+id/dashboard_led"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@mipmap/u6"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_f"
        app:layout_constraintEnd_toStartOf="@+id/dashboard_f"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/dashboard_f"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/dashboard_f"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@mipmap/u7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="24dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageView
        android:id="@+id/dashboard_cl"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:src="@mipmap/u15"
        app:layout_constraintBottom_toBottomOf="@+id/dashboard_f"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/dashboard_f"
        app:layout_constraintTop_toTopOf="@+id/dashboard_f"
        tools:ignore="ContentDescription" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/SwitchCompat_led"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_led"
        app:layout_constraintStart_toStartOf="@+id/dashboard_led"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_led"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/SwitchCompat_fan"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_f"
        app:layout_constraintStart_toStartOf="@+id/dashboard_f"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_f" />

    <androidx.appcompat.widget.SwitchCompat
        android:id="@+id/SwitchCompat_curtain"
        android:layout_width="48dp"
        android:layout_height="48dp"
        app:layout_constraintEnd_toEndOf="@+id/dashboard_cl"
        app:layout_constraintStart_toStartOf="@+id/dashboard_cl"
        app:layout_constraintTop_toBottomOf="@+id/dashboard_cl" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/SwitchCompat_fan"
        tools:layout_editor_absoluteX="0dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/FloatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:src="@drawable/baseline_add_24"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />


</androidx.constraintlayout.widget.ConstraintLayout>